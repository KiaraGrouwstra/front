  "scripts": {
    "start": "node server.js",
    "postinstall": "typings install",
    "tsc": "tsc -m commonjs -t es5 --emitDecoratorMetadata ./app/app.ts --experimentalDecorators --outDir ./dist/",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build": "rm -rf dist && tsc -p src",
    "update": "npm update update && typings update",
    "prod": "webpack -p # minifies",
    "maps": "webpack -d # source maps",
    "index": "pug ./app/index.pug -o ./dist/",
    "watch": "webpack -w",
    "dev": "webpack-dev-server",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html"
  },
