//- {k, id, named, cols: [{k, id}], rows: [{id, cells: [{id, val}]}]}
.card
  .card-content
    - if(named)
      span.card-title= k
    table.bordered.highlight(id=id)
      thead
        - each col in cols
          th(id=col.id)= col.k
      tbody
        - each r in rows
          tr(id=r.id)
            - each c in r.cells
              td(id=c.id)!= c.val
